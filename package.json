{
  "name": "annuity-value-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Client/server annuity calculator with Fastify, Vue, and Vuetify",
  "scripts": {
    "server": "node server.js",
    "start": "node server.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "db:migrate": "knex --knexfile knexfile.cjs migrate:latest",
    "db:rollback": "knex --knexfile knexfile.cjs migrate:rollback",
    "db:seed": "knex --knexfile knexfile.cjs seed:run",
    "db:seed:users": "knex --knexfile knexfile.cjs seed:run --specific=01_user_profiles_seed.cjs",
    "db:seed:calculator": "knex --knexfile knexfile.cjs seed:run --specific=02_calculation_logs_seed.cjs",
    "test": "node --test",
    "test:client": "vitest run",
    "test:client:coverage": "vitest run --coverage",
    "test:coverage": "c8 node --test",
    "test:coverage:all": "c8 --config .c8rc.json --check-coverage false --per-file false --reporter text-summary node --test",
    "test:coverage:full": "npm run test:coverage && npm run test:client:coverage",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.7.0",
    "@fastify/swagger-ui": "^5.2.5",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@mdi/font": "^7.4.47",
    "@sinclair/typebox": "^0.34.41",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/vue-query": "^5.90.5",
    "@tanstack/vue-router": "^1.159.10",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.1.1",
    "fastify": "^5.7.4",
    "fastify-plugin": "^5.0.1",
    "jose": "^6.1.0",
    "knex": "https://codeload.github.com/knex/knex/tar.gz/refs/heads/master",
    "mysql2": "^3.15.3",
    "pinia": "^3.0.4",
    "vue": "^3.5.13",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "c8": "^10.1.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "prettier": "^3.6.2",
    "vite": "^6.1.0",
    "vitest": "^4.0.18"
  }
}
