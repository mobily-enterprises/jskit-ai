{
  "name": "annuity-value-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Client/server annuity calculator with Fastify, Vue, and Vuetify",
  "scripts": {
    "server": "node bin/server.js",
    "start": "node bin/server.js",
    "dev": "VITE_CLIENT_ENTRY=main.js vite",
    "build": "VITE_CLIENT_ENTRY=main.js vite build --mode internal",
    "build:client:internal": "VITE_CLIENT_ENTRY=main.js vite build --mode internal",
    "build:client:public": "VITE_CLIENT_ENTRY=main.public.js vite build --mode public",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "db:migrate": "knex --knexfile knexfile.cjs migrate:latest",
    "db:rollback": "knex --knexfile knexfile.cjs migrate:rollback",
    "db:seed": "knex --knexfile knexfile.cjs seed:run",
    "db:seed:users": "knex --knexfile knexfile.cjs seed:run --specific=01_user_profiles_seed.cjs",
    "db:seed:calculator": "knex --knexfile knexfile.cjs seed:run --specific=02_calculation_logs_seed.cjs",
    "test": "NODE_ENV=test node --test",
    "test:client": "vitest run",
    "test:client:coverage": "vitest run --coverage",
    "test:client:views": "vitest run --config vitest.vue.config.mjs",
    "test:client:views:coverage": "vitest run --config vitest.vue.config.mjs --coverage",
    "test:coverage": "NODE_ENV=test c8 --config .c8rc.json node --test",
    "test:coverage:all": "NODE_ENV=test c8 --config .c8rc.json --check-coverage false --per-file false --reporter text-summary node --test",
    "test:coverage:aspirational": "NODE_ENV=test c8 --config .c8rc.aspirational.json --check-coverage false --per-file false --reporter text-summary node --test",
    "test:coverage:full": "npm run docs:api-contracts:check && npm run test:coverage && npm run test:client:coverage && npm run test:client:views:coverage",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "docs:api-contracts": "node bin/syncApiContractsReadme.js",
    "docs:api-contracts:check": "node bin/syncApiContractsReadme.js --check"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.7.0",
    "@fastify/swagger-ui": "^5.2.5",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@mdi/js": "^7.4.47",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/vue-query": "^5.90.5",
    "@tanstack/vue-router": "^1.159.10",
    "@uppy/compressor": "^3.1.0",
    "@uppy/core": "^5.2.0",
    "@uppy/dashboard": "^5.1.1",
    "@uppy/image-editor": "^4.2.0",
    "@uppy/xhr-upload": "^5.1.1",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.1.1",
    "fastify": "^5.7.4",
    "fastify-plugin": "^5.0.1",
    "jose": "^6.1.0",
    "knex": "https://codeload.github.com/knex/knex/tar.gz/refs/heads/master",
    "mysql2": "^3.15.3",
    "pinia": "^3.0.4",
    "sharp": "^0.34.4",
    "unstorage": "^1.17.0",
    "vue": "^3.5.13",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "c8": "^10.1.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "prettier": "^3.6.2",
    "vite": "^6.1.0",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.18"
  }
}
