{
  "version": 1,
  "defaultInviteRole": "member",
  "roles": {
    "owner": {
      "assignable": false,
      "permissions": ["*"]
    },
    "admin": {
      "assignable": true,
      "permissions": [
        "workspace.members.invite",
        "workspace.members.view",
        "workspace.members.manage",
        "workspace.invites.revoke",
        "workspace.settings.view",
        "workspace.settings.update",
        "workspace.roles.view",
        "history.read",
        "history.write",
        "projects.read",
        "projects.write"
      ]
    },
    "member": {
      "assignable": true,
      "permissions": ["history.read", "history.write", "projects.read", "projects.write"]
    },
    "viewer": {
      "assignable": true,
      "permissions": ["history.read", "projects.read"]
    }
  }
}
